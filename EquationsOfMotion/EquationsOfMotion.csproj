<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Scott Brettle</Authors>
    <Description>A collection of methods for solving Displacement, Initial and Final Velocities, Constant Acceleration and Time.</Description>
    <PackageProjectUrl>https://github.com/srbrettle/Equations-Of-Motion-Library</PackageProjectUrl>
    <RepositoryUrl>https://github.com/srbrettle/Equations-Of-Motion-Library</RepositoryUrl>
    <PackageTags>physics, equations, motion, kinematic, displacement, velocity, acceleration, time</PackageTags>
    <PackageIconUrl>https://raw.githubusercontent.com/srbrettle/Equations-Of-Motion-Library/master/EquationsOfMotion.png</PackageIconUrl>
    <PackageLicenseUrl>https://github.com/srbrettle/Equations-Of-Motion-Library/blob/master/LICENSE</PackageLicenseUrl>
    <Version>1.0.0</Version>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DebugType>full</DebugType>
    <DebugSymbols>true</DebugSymbols>
  </PropertyGroup>

</Project>
